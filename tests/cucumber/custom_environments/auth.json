{
    "dirs_config": {
        "appmock": {
            "input_dir": "rel/appmock",
            "target_dir": "rel/test_cluster"
        },
        "cluster_manager": {
            "input_dir": "rel/cluster_manager",
            "target_dir": "rel/test_cluster"
        },
        "op_worker": {
            "input_dir": "rel/op_worker",
            "target_dir": "rel/test_cluster"
        },
        "cluster_worker": {
            "input_dir": "rel/cluster_worker",
            "target_dir": "rel/test_cluster"
        },
        "oz_worker": {
            "input_dir": "rel/oz_worker",
            "target_dir": "rel/test_cluster"
        },
        "onepanel": {
            "input_dir": "rel_oneprovider/onepanel",
            "target_dir": "rel/test_cluster"
        }
    },
    "appmock_domains": {
        "openid_server": {
            "appmock": {
                "openid": {
                    "vm.args": {
                        "setcookie": "cookie"
                    },
                    "sys.config": {
                        "appmock": {
                            "app_description_file": "./appmock_openid_server.erl"
                        }
                    }
                }
            }
        }
    },
    "os_configs": {
        "cfg1": {
            "storages": [
                {
                    "type": "posix",
                    "name": "/mnt/st1"
                }
            ],
            "users": [
                "u1",
                "u2"
            ],
            "groups": {
                "s1": [
                    "u1",
                    "u2"
                ],
                "s2": [
                    "u1",
                    "u2"
                ],
                "fuse": [
                    "u1",
                    "u2"
                ]
            }
        }
    },
    "p1": {
        "db_driver": "couchdb",
        "os_config": "cfg1",
        "cluster_manager": {
            "cm1": {
                "vm.args": {
                    "setcookie": "cookie2"
                },
                "sys.config": {
                    "cluster_manager": {
                        "cm_nodes": [
                            "cm1"
                        ],
                        "worker_num": 1
                    }
                }
            }
        },
        "op_worker": {
            "worker1": {
                "vm.args": {
                    "setcookie": "cookie2"
                },
                "sys.config": {
                    "op_worker": {
                        "cm_nodes": [
                            "cm1"
                        ],
                        "db_nodes": [
                            "dbnode1"
                        ],
                        "oz_domain": "oz1",
                        "verify_oz_cert": false
                    }
                },
                "storage": {
                    "storage1": {
                        "volume_path": "/media/storage",
                        "host_path": "/tmp/blah"
                    }
                }
            }
        }
    },
    "zone_domains": {
        "oz1": {
            "db_driver": "couchdb",
            "cluster_manager": {
                "cm": {
                    "vm.args": {
                        "setcookie": "cookie4"
                    },
                    "sys.config": {
                        "cluster_manager": {
                            "cm_nodes": [
                                "cm"
                            ],
                            "worker_num": 1
                        }
                    }
                }
            },
            "oz_worker": {
                "node1": {
                    "vm.args": {
                        "setcookie": "cookie4"
                    },
                    "sys.config": {
                        "oz_worker": {
                            "cm_nodes": [
                                "cm"
                            ],
                            "db_nodes": [
                                "127.0.0.1:49161"
                            ],
                            "grpcert_domain": {
                                "string": "127.0.0.1"
                            },
                            "dev_mode": true
                        }
                    }
                }
            }
        }
    },
    "client-host1": {
        "os_config": "cfg1",
        "clients": {
            "client1": {
                "name": "client1",
                "op_domain": "p1",
                "zone_domain": "oz1",
                "user_cert": "./cert.pem",
                "user_key": "./cert.pem",
                "mounting_path": "/home/u1/onedata",
                "token_for": "u1",
                "default_timeout": 30
            },
            "client2": {
                "name": "client2",
                "op_domain": "p1",
                "zone_domain": "oz1",
                "user_cert": "./cert.pem",
                "user_key": "./cert.pem",
                "mounting_path": "/home/u2/onedata",
                "token_for": "u2",
                "default_timeout": 30
            }
        }
    },
    "global_setup": {
        "users": {
            "u1": {
                "default_space": "s1"
            },
            "u2": {
                "default_space": "s1"
            }
        },
        "groups": {
            "g1": {
                "users": [
                    "u1",
                    "u2"
                ]
            }
        },
        "spaces": {
            "s1": {
                "displayed_name": "s1",
                "users": [
                    "u1",
                    "u2"
                ],
                "groups": [
                    "g1"
                ],
                "providers": {
                    "p1": {
                        "storage": "/mnt/st1",
                        "supported_size": 1000000000
                    }
                }
            },
            "s2": {
                "displayed_name": "s2",
                "users": [
                    "u1",
                    "u2"
                ],
                "groups": [
                    "g1"
                ],
                "providers": {
                    "p1": {
                        "storage": "/mnt/st1",
                        "supported_size": 1000000000
                    }
                }
            }
        }
    }

}