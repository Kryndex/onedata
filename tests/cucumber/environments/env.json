{
  "dirs_config": {
    "op_ccm": {
      "input_dir": "rel/op_ccm",
      "target_dir": "rel/test_cluster"
    },
    "op_worker": {
      "input_dir": "rel/op_worker",
      "target_dir": "rel/test_cluster"
    },
    "globalregistry": {
      "input_dir": "rel/globalregistry",
      "target_dir": "rel/test_cluster"
    },
    "onepanel": {
      "input_dir": "rel_oneprovider/onepanel",
      "target_dir": "rel/test_cluster"
    },
    "oneclient": {
      "input_dir": "rel/oneclient",
      "target_dir": "rel/test_cluster"
    }
  },
  "os_configs": {
    "cfg1": {
      "storages": [
        "/mnt/st1",
        "/mnt/st2"
      ],
      "users": [
        "u1",
        "u2",
        "u3"
      ],
      "groups": {
        "s1": [
          "u1",
          "u2"
        ],
        "s2": [
          "u1",
          "u2"
        ],
        "fuse": [
          "u1",
          "u2"
        ]
      }
    }
  },
  "provider_domains": {
    "p1": {
      "db_driver": "riak",
      "os_config": "cfg1",
      "op_ccm": {
        "ccm1": {
          "vm.args": {
            "setcookie": "cookie2"
          },
          "sys.config": {
            "ccm_nodes": [
              "ccm1"
            ],
            "worker_num": 1
          }
        }
      },
      "op_worker": {
        "worker1": {
          "vm.args": {
            "setcookie": "cookie2"
          },
          "sys.config": {
            "ccm_nodes": [
              "ccm1"
            ],
            "db_nodes": [
              "riaknode1"
            ],
            "global_registry_domain": "gr",
            "verify_gr_cert": false
          },
          "storage": {
            "storage1": {
              "volume_path": "/media/storage",
              "host_path": "/tmp/blah"
            }
          }
        }
      }
    }
  },
  "globalregistry_domains": {
    "gr": {
      "globalregistry": {
        "node1": {
          "vm.args": {
            "setcookie": "cookie3"
          },
          "sys.config": {
            "db_nodes": [
              "db"
            ],
            "grpcert_domain": {
              "string": "127.0.0.1"
            },
            "dev_mode": true
          }
        }
      }
    }
  },
  "global_setup": {
    "users": {
      "u1": {
        "default_space": "s1"
      },
      "u2": {
        "default_space": "s1"
      }
    },
    "groups": {
      "g1": {
        "users": [
          "u1",
          "u2"
        ]
      }
    },
    "spaces": {
      "s1": {
        "displayed_name": "s1",
        "users": [
          "u1",
          "u2"
        ],
        "groups": [
          "g1"
        ],
        "providers": [
          {
            "provider": "p1",
            "supported_size": 1000000000
          }
        ]
      },
      "s2": {
        "displayed_name": "s2",
        "users": [
          "u1",
          "u2"
        ],
        "groups": [
          "g1"
        ],
        "providers": [
          {
            "provider": "p1",
            "supported_size": 10000
          }
        ]
      }
    }
  },
  "oneclient": {
    "1": {
      "os_config": "cfg1",
      "clients": {
        "client1": {
          "name": "client1",
          "op_domain": "p1",
          "gr_domain": "gr",
          "user_cert": "./cert.pem",
          "user_key": "./cert.pem",
          "mounting_path": "/home/u1/onedata",
          "token_for": "u1"
        },
        "client2": {
          "name": "client2",
          "op_domain": "p1",
          "gr_domain": "gr",
          "user_cert": "./cert.pem",
          "user_key": "./cert.pem",
          "mounting_path": "/home/u1/onedata2",
          "token_for": "u1"
        }
      }
    },
    "2": {
      "os_config": "cfg1",
      "clients": {
        "client1": {
          "name": "client1",
          "op_domain": "p1",
          "gr_domain": "gr",
          "user_cert": "./cert.pem",
          "user_key": "./cert.pem",
          "mounting_path": "/home/u1/onedata",
          "token_for": "u1"
        },
        "client2": {
          "name": "client2",
          "op_domain": "p1",
          "gr_domain": "gr",
          "user_cert": "./cert.pem",
          "user_key": "./cert.pem",
          "mounting_path": "/home/u2/onedata",
          "token_for": "u2"
        }
      }
    }
  }
}

