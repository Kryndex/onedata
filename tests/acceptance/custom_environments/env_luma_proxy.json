{
    "dirs_config": {
        "cluster_manager": {
            "input_dir": "_build/default/rel/cluster_manager",
            "target_dir": "_build/default/rel/test_cluster"
        },
        "op_worker": {
            "input_dir": "_build/default/rel/op_worker",
            "target_dir": "_build/default/rel/test_cluster"
        },
        "cluster_worker": {
            "input_dir": "_build/default/rel/cluster_worker",
            "target_dir": "_build/default/rel/test_cluster"
        },
        "oz_worker": {
            "input_dir": "_build/default/rel/oz_worker",
            "target_dir": "_build/default/rel/test_cluster"
        },
        "onepanel": {
            "input_dir": "_build/default/rel/onepanel",
            "target_dir": "_build/default/rel/test_cluster"
        }
    },
    "os_configs": {
        "cfg1": {
            "storages": [
                {
                  "type": "posix",
                  "name": "/mnt/st1"
                },
                {
                  "type": "ceph",
                  "name": "ceph",
                  "pool": "onedata:8"
                },
                {
                  "type": "s3",
                  "name": "s3",
                  "bucket": "onedata",
                  "disable_iam_mock": true
                },
                {
                  "type": "swift",
                  "name": "swift",
                  "container": "swift"
                }
            ],
            "users": [
                "u1"
            ],
            "groups": {
                "ceph": [
                    "u1"
                ],
                "posix": [
                    "u1"
                ],
                "s3": [
                    "u1"
                ],
                "fuse": [
                    "u1"
                ]
            }
        }
    },
    "provider_domains": {
        "p1": {
            "db_driver": "couchdb",
            "os_config": "cfg1",
            "enable_luma_proxy": true,
            "cluster_manager": {
                "cm1": {
                    "vm.args": {
                        "setcookie": "cookie2"
                    },
                    "sys.config": {
                        "cluster_manager": {
                            "cm_nodes": [
                                "cm1"
                            ],
                            "worker_num": 1
                        }
                    }
                }
            },
            "op_worker": {
                "worker1": {
                    "vm.args": {
                        "setcookie": "cookie2"
                    },
                    "sys.config": {
                        "op_worker": {
                            "cm_nodes": [
                                "cm1"
                            ],
                            "db_nodes": [
                                "dbnode1"
                            ],
                            "oz_domain": "oz1",
                            "verify_oz_cert": false
                        }
                    }
                }
            }
        }
    },
    "zone_domains": {
        "oz1": {
            "db_driver": "couchdb",
            "cluster_manager": {
                "cm": {
                    "vm.args": {
                        "setcookie": "cookie3"
                    },
                    "sys.config": {
                        "cluster_manager": {
                            "cm_nodes": [
                                "cm"
                            ],
                            "worker_num": 1
                        }
                    }
                }
            },
            "oz_worker": {
                "node1": {
                    "vm.args": {
                        "setcookie": "cookie3"
                    },
                    "sys.config": {
                        "oz_worker": {
                            "cm_nodes": [
                                "cm"
                            ],
                            "db_nodes": [
                                "127.0.0.1:49161"
                            ],
                            "grpcert_domain": {
                                "string": "127.0.0.1"
                            },
                            "dev_mode": true
                        }
                    }
                }
            }
        }
    },
    "oneclient": {
        "client-host1": {
            "os_config": "cfg1",
            "clients": {
                "client1": {
                    "name": "client1",
                    "op_domain": "p1",
                    "zone_domain": "oz1",
                    "user_cert": "./cert.pem",
                    "user_key": "./cert.pem",
                    "mounting_path": "/home/u1/onedata",
                    "token_for": "u1",
                    "default_timeout": 5
                }
            }
        }
    },
    "global_setup": {
        "users": {
            "u1": {
                "default_space": "posix"
            }
        },
        "groups": {
            "g1": {
                "users": [
                    "u1"
                ]
            }
        },
        "spaces": {
            "ceph": {
                "displayed_name": "ceph",
                "users": [
                    "u1"
                ],
                "groups": [
                    "g1"
                ],
                "providers": {
                    "p1": {
                        "storage": "ceph",
                        "supported_size": 1000000000
                    }
                }
            },
            "s3": {
                "displayed_name": "s3",
                "users": [
                    "u1"
                ],
                "groups": [
                    "g1"
                ],
                "providers": {
                    "p1": {
                        "storage": "s3",
                        "supported_size": 1000000000
                    }
                }
            },
            "posix": {
                "displayed_name": "posix",
                "users": [
                    "u1"
                ],
                "groups": [
                    "g1"
                ],
                "providers": {
                    "p1": {
                        "storage": "/mnt/st1",
                        "supported_size": 1000000000
                    }
                }
            },
            "swift": {
                "displayed_name": "swift",
                "users": [
                    "u1"
                ],
                "groups": [
                    "g1"
                ],
                "providers": {
                    "p1": {
                        "storage": "swift",
                        "supported_size": 1000000000
                    }
                }
            }
        }
    }
}
